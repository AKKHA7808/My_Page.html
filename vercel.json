{
  "version": 2,
  "builds": [
    {
      "src": "portfolio/wsgi.py",
      "use": "@vercel/python",
      "config": { 
        "maxLambdaSize": "15mb", 
        "runtime": "python3.11"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "portfolio/wsgi.py"
    }
  ],
  "env": {
    "DEBUG": "False",
    "DJANGO_ENV": "production"
  },
  "build": {
    "env": {
      "PYTHONPATH": "$PYTHONPATH:."
    }
  },
  "functions": {
    "portfolio/wsgi.py": {
      "includeFiles": "**"
    }
  }
}
